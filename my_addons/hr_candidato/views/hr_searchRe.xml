<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="searchre_list">
      <field name="name">Model Search of References</field>
      <field name="model">model.searchre</field>
      <field name="arch" type="xml">
        <tree>
          <field name="applicant_id"/>
          <field name="informative_interest"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="searchre_action_window">
      <field name="name">Model Search of References</field>
      <field name="res_model">model.searchre</field>
      <field name="view_mode">tree,form</field>
    </record>
  </data>
</odoo>