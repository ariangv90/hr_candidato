<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.module.category" id="module_category_hr_candidato">
            <field name="name">Process Selection</field>
            <field name="description">Help, Administration  for Modulate Candidate's Request.</field>
            <field name="sequence">16</field>
        </record>
        <record id="group_hr_candidato_user" model="res.groups">
            <field name="name">Office</field>
            <field name="category_id" ref="hr_candidato.module_category_hr_candidato"/>
            <field name="implied_ids" eval="[(4, ref('hr.group_hr_user'))]"/>
        </record>
        <record id="group_hr_candidato_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="hr_candidato.module_category_hr_candidato"/>
            <field name="implied_ids" eval="[(4, ref('hr_candidato.group_hr_candidato_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>

        <record id="base.default_user" model="res.users">
        <field name="groups_id" eval="[(4,ref('hr_candidato.group_hr_candidato_manager'))]"/>
    </record>
    </data>
</odoo>